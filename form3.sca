	PROCEDURE Destroy
		*ON KEY LABEL F9
		POP KEY 
		SELECT (al)
		
	ENDPROC

	PROCEDURE Init
		  PUSH KEY CLEAR 
		
		  
		*  BROWSE 
		*  ON KEY LABEL F9 DO p_lst
		  fr_2.Grid1.Visible = .F.
		  fr_2.Label1.Caption = 'Для визуализации таблицы БИК закройте просмотр.'
		*  =MESSAGEBOX(STR(fr_2.Label1.Left,12))
		  
		  
		  ThisForm.Width = SYSMETRIC(1)/5.03  
		  ThisForm.Height = 79/19.25*(SYSMETRIC(2)/100)
		
		  ThisForm.Grid1.ColumnCount = 2
		  ThisForm.Grid1.Width = SYSMETRIC(1)/5.03 && Ширина Grid1 
		  ThisForm.Grid1.Height = 79/19.25*(SYSMETRIC(2)/100) && Высота грид  
		*  ThisForm.Grid1.AutoFit()
		  
		
		  ThisForm.Grid1.Column1.ControlSource = 'p807.pNames'
		  ThisForm.Grid1.Column1.Header1.Caption = 'Название атрибутов'
		  ThisForm.Grid1.Column1.Width = 35/7*(SYSMETRIC(1)/100) 
		
		  ThisForm.Grid1.Column2.ControlSource = 'p807.pZnach'
		  ThisForm.Grid1.Column2.Header1.Caption = 'Значения'
		  ThisForm.Grid1.Column2.Width = SYSMETRIC(1)/7 
		 
		  fr_2.Label1.Left = fr_2.Grid2.Width+6/7.5*(SYSMETRIC(1)/100)
		  fr_2.Label1.Top  = fr_2.Grid1.Height+6/19.25*(SYSMETRIC(2)/100)
		
		* =MESSAGEBOX(' Left= '+STR(fr_2.Label1.Left,12)+'   Top= '+STR(fr_2.Label1.Top,12))
	ENDPROC

	PROCEDURE Load
		  SELECT 3
		  CREATE CURSOR p807 (pNames C(100), pZnach C(254))
		
		  FOR s=1 TO fr_2.Grid1.ColumnCount
		   lctmp = 'fr_2.Grid1.Column'+ALLTRIM(STR(s))+'.Header1.Caption'
		*   lcftmp='a807.'+FIELD(s,'a807')
		   lcftmp = 'fr_2.Grid1.Column'+ALLTRIM(STR(s))+'.ControlSource'
		   lcftmp2 = &lcftmp
		   APPEND BLANK 
		   REPLACE pNames WITH &lctmp, pZnach WITH &lcftmp2
		
		  ENDFOR  
		
		  GO TOP 
	ENDPROC

	PROCEDURE QueryUnload
		 FOR lnForCounter = 1 TO _Screen.FormCount 
		 IF UPPER(_Screen.Forms(lnForCounter).Name) = 'FORM2' && Если форма есть в массиве _Screen.Forms()
		  IF TYPE('_SCREEN.FORMS(lnForCounter).NAME') = 'C' && Если _Screen.ActiveForm в данный момент является объектом и на неё можно ссылаться 
		
		  fr_2.Grid1.Visible = .T.
		  fr_2.Label1.Caption = ''
		     
		  ENDIF
		 ENDIF
		 ENDFOR
		
	ENDPROC

	PROCEDURE Grid1.Column1.Text1.MouseDown
		LPARAMETERS nButton, nShift, nXCoord, nYCoord
